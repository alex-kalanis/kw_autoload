{
    "name": "alex-kalanis/kw_autoload",
    "description": "Autoloader for KWCMS - this is just for tests and code styles",
    "keywords": [
        "autoloader"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "psr/container": "=2.0.2",
        "phpunit/phpunit": ">=7.2 <=9"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_autoload\\": "src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
